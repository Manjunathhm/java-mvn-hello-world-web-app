pipeline {
  agent { label 'slave1' }
  stages{
    stage('Checkout'){
      steps{
        sh 'rm -r /home/slave1/workspace/pipelineProject1/java-mvn-hello-world-web-app'
        sh 'git clone https://github.com/manjunathhm/java-mvn-hello-world-web-app.git'
        sh 'cd java-mvn-hello-world-web-app'
        sh 'pwd'
        sh 'cd /home/slave1/workspace/pipelineProject1/java-mvn-hello-world-web-app'
        sh 'pwd'
        sh 'mvn clean install'
        sh 'pwd'
        sh 'ls -l'
      }
  }
  }
 }
